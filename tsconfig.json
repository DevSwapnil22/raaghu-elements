/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "paths": {
      "rds-add-new-box": [
        "dist/rds-elements/rds-add-new-box"
      ],
      "rds-accordian": [
        "dist/rds-elements/rds-accordian"
      ],
      "rds-alert": [
        "dist/rds-elements/rds-alert"
      ],
      "rds-app-config": [
        "dist/rds-elements/rds-app-config"
      ],
      "rds-chart-area": [
        "dist/rds-elements/rds-chart-area"
      ],
      "rds-avatar": [
        "dist/rds-elements/rds-avatar"
      ],
      "rds-badge": [
        "dist/rds-elements/rds-badge"
      ],
      "rds-banner": [
        "dist/rds-elements/rds-banner"
      ],
      "rds-big-number-widget": [
        "dist/rds-elements/rds-big-number-widget"
      ],
      "rds-chart-bool": [
        "dist/rds-elements/rds-chart-bool"
      ],
      "rds-breadcrumb": [
        "dist/rds-elements/rds-breadcrumb"
      ],
      "rds-chart-bubble": [
        "dist/rds-elements/rds-chart-bubble"
      ],
      "rds-button": [
        "dist/rds-elements/rds-button"
      ],
      "rds-button-group": [
        "dist/rds-elements/rds-button-group"
      ],
      "rds-card": [
        "dist/rds-elements/rds-card"
      ],
      "rds-carousel": [
        "dist/rds-elements/rds-carousel"
      ],
      "rds-chart-bar-horizontal": [
        "dist/rds-elements/rds-chart-bar-horizontal"
      ],
      "rds-chart-doughnut": [
        "dist/rds-elements/rds-chart-doughnut"
      ],
      "rds-checkbox": [
        "dist/rds-elements/rds-checkbox"
      ],
      "rds-input": [
        "dist/rds-elements/rds-input"
      ]
    },
    "target": "es2017",
    "module": "es2020",
    "lib": [
      "es2020",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
